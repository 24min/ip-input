        "vue",
        "vue2",
        "ip",
        "input",
        "fan",
        "frontend"
    ],
    "main": "lib/fanIpinput.umd.min.js",
    "license": "MIT",
    "repository": {
        "type": "git",
        "uri": "https://github.com/24min/ip-input"
    },
    "bugs": {
        "url": "https://github.com/24min/ip-input/issues"
    },
